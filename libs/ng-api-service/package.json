{
  "name": "@codegena/ng-api-service",
  "description": "Extension for @codegena/oapi3ts: Angular services for work with REST API",
  "version": "2.1.5",
  "author": "koshevy@gmail.com",
  "keywords": [
    "swagger",
    "oas",
    "oas3",
    "oas 3",
    "openapi",
    "openapi3",
    "open-api",
    "open api",
    "codegeneration",
    "code generation",
    "typescript",
    "angular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/koshevy/codegena.git"
  },
  "license": "MIT",
  "scripts": {
