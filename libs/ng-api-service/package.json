    "lodash": ">= 4"
  },
  "devDependencies": {
    "jest-preset-angular": ">= 7",
    "@types/lodash": ">= 4",
    "@types/node": ">= 8"
  },
  "optionalDependencies": {
    "@angular/common": ">= 6",
    "@angular/core": ">= 6",
    "@angular/platform-browser-dynamic": ">= 6",
    "rxjs": ">= 6",
    "core-js": "*",
    "zone.js": ">= 0"
  }
}
